{
    "name": "djimmy/laravel-captcha",
    "description": "A simple Laravel package for generating CAPTCHA images.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Djimmy Poliard",
            "email": "djimmypoliard@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0"
